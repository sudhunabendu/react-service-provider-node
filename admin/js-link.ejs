<script>
  $(document).ready(function () {
    $("#signupform").validate({
      rules: {
        p_name: "required",
        p_slug: "required",
        p_desc: "required",
        price: "required",
        qty: "required",
        color: "required",
        size: "required",
        cat_id: "required",
        image: "required",
      },
      messages: {
        p_name: "Please enter product name",
        p_slug: "Please enter product slug",
        p_desc: "Please enter product description",
        price: "Please enter product price",
        qty: "Please enter product qty",
        color: "Please enter product color",
        size: "Please enter product size",
        cat_id: "Please select category",
        image: "Please upload product image",
      }
    })
  })
</script>




<script>
  $(document).ready(function () {
    $('input[name=toogle]').change(function () {
      var status = $(this).prop('checked') == true ? 1 : 0;
      var id = $(this).val();
      //alert(status);
      $.post("/admin/accstatus", { status: status, id: id }, function (data) {
        if (data === 'success') {
          alert("success");
          window.location.href = "/admin/users";
        } else {
          alert("somthing error");
          window.location.href = "/admin/users";
        }
      });
    });

  });
</script>

<script>
  $(document).ready(function () {
    $('.only_numbers').keyup(function () {
      var numbers = $(this).val();
      $(this).val(numbers.replace(/\D/, ''));
    });
  });
</script>



